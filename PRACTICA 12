// C++ code
//
void setup()
{
  pinMode(12, OUTPUT);
  pinMode(11, OUTPUT);
}

void loop()
{
  digitalWrite(12, HIGH);
  digitalWrite(11, HIGH);  // aqui las dos encendidas
  delay(10000); // Wait for 10000 millisecond(s)
  digitalWrite(12, LOW); // aqui las dos apagadas
  digitalWrite(11, LOW);
  delay(4000); // Wait for 4000 millisecond(s)
}
